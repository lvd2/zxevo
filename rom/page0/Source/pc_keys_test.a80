PC_KEY_X	EQU 1				;┼▌▌░└┬█─▓─ ┌⌡┌▌└─ ░─▄┼┬
PC_KEY_Y	EQU 5				;┼▌▌░└┬█─▓─ ┌⌡┌▌└─ ░─▄┼┬
PRT_PCKEY_X	EQU 16
PRT_PCKEY_Y	EQU 22
PR_COL		EQU %00111000
UPR_COL		EQU %01000100

PC_KEYS_TEST	DI
		LD A,%01000010
		CALL CLEAR_TXTSCR
		CALL TXTMODE_ON
		LD DE,PC_KEY_X*0X100+PC_KEY_Y
		LD BC,0X4E10
		CALL RAMKA4TXTSCR
		LD HL,PC_KEYS
		CALL PRT4TXTMODE
		LD DE,PRT_PCKEY_X*0X100+PRT_PCKEY_Y
		CALL PRINT_EMPTY
		LD HL,TXT4TESTERPC
		CALL PRT4TXTMODE
		CALL SYSTEM_KEYS
		LD HL,0XC000
		LD DE,0XC001
		LD BC,0XFF
		LD (HL),L
		LDIR
PKT_05		LD H,0X0C
		CALL READCMOS			;▐░▌≈┬▓─▀┬ ░┘┐┬▒▓░ 0X0C CMOS
		SET 0,L				;│┬▓ 0=1 ▒│░▌▒ │⌠■┘░─ ┼▀─┌┬─▓⌠░⌡
		CALL WRITECMOS			;┤─▐┬▒─▀┬ ▌│░─▓█▌
		LD HL,0XF002
		CALL WRITECMOS			;┌┼▀·≈┬▀┬ └▌▒▓⌠▐ ┼ │⌠■┘░⌠ ┼▀─┌┬─▓⌠░⌡
		LD DE,PRT_PCKEY_X*0X100+PRT_PCKEY_Y
		LD B,0
PKT_04		EI
		HALT
		DI
		CALL KEYS4EXIT
		JR NZ,EXIT_TESTER
		LD H,0XF0
		CALL READCMOS			;≈▓┘█┬┘ │─┴▓─ ┬┤ │⌠■┘░─ ┼▀─┌┬─▓⌠░⌡
		AND A
		JR Z,PKT_04			;┘▒▀┬ █▌▀° ▓▌ ├└┘▄
		INC A
		JR Z,PKT_05			;┘▒▀┬ FF ▓▌ ┬█┬▓┬▄ ▀▌┐
		LD A,L
		CP 0XE0
		JR NZ,PKT_06
		SET 7,B				;█─├─▓─ ┼█▌▐┼─ ▒ └▌▐ ┼▌└▌▄
		JR PKT_0D

PKT_06		CP 0XF0
		JR NZ,PKT_07
		SET 6,B				;▌▓▐⌠≥┘█─ ┼▀─┌┬≤─
		JR PKT_0D

PKT_07		CP 0X58
		CALL Z,CAPSLOCKLED
		LD H,0XC0
		LD C,1				;█─├─▓─ ┼█▌▐┼─
		BIT 7,B
		JR Z,PKT_0C
		LD C,2				;█─├─▓─ ┼█▌▐┼─ ▒ └▌▐ ┼▌└▌▄
		LD A,0XE0
		CALL PRTT_A_
		LD A,","
		CALL PRTSYMTXTMODE
PKT_0C		BIT 6,B
		JR Z,PKT_08
		LD C,3				;▌▓▐⌠≥┘█─ ┼█▌▐┼─
		LD A,0XF0
		CALL PRTT_A_
		LD A,","
		CALL PRTSYMTXTMODE
PKT_08		LD (HL),C
		LD A,L
		CALL PRTT_A_
		CALL DRAW_COLOR
		LD A,3
		LD B,0
		SUB C
		JR NZ,PKT_00
		LD H,0XC0
		LD (HL),B
PKT_00		CALL PRINT_EMPTY		;▐┘≈─▓° ▐⌠▒▓▌┴ ▒▓░▌┼┬
PKT_0D		LD DE,PRT_PCKEY_X*0X100+PRT_PCKEY_Y
		JR PKT_04

CAPSLOCKLED	LD A,B
		AND 0XC0
		RET NZ
		PUSH HL
		LD H,0X0C
		CALL READCMOS
		LD A,2
		XOR L
		LD L,A
		CALL WRITECMOS
		POP HL
		RET

EXIT_TESTER	CALL TXTMODE_OFF
		LD A,0X10
		CALL SET_7FFD
		JP RESTART

KEYS4EXIT	LD HL,0XC011
		LD A,(HL)
		AND A
		RET Z
		LD L,0X0C
		LD A,(HL)
		AND A
		RET

;░┬▒▌┌─█┬┘ √┌┘▓█▌┐▌ ┼┌─└░─▓┬┼─ █─├─▓▌┴ ┬▀┬ ▌▓▐⌠≥┘█▌┴ ┼█▌▐┼┬
DRAW_COLOR	PUSH BC
		PUSH DE
		PUSH HL
		BIT 7,B
		LD DE,TABL_PR_COL		;▓─│▀┬√─ ▌▒█▌┌█⌡∙ ┼▌└▌┌
		JR Z,DRAWC1
		LD DE,EXTTBL_PR_COL		;▓─│▀┬√─ ┼▌└▌┌ ▒ 0XE0
DRAWC1		LD B,(HL)			;┌┤÷▀┬ ┼▌└ ┼█▌▐┼┬
		LD H,0
		ADD HL,HL
		ADD HL,HL
		ADD HL,DE
		LD D,(HL)			;┼▌▌░└┬█─▓─ X
		INC D
		INC D
		INC HL
		LD E,(HL)			;┼▌▌░└┬█─▓─ Y
		INC E
		INC HL
		LD C,(HL)			;┌⌡▒▌▓─
		INC HL
		LD A,C
		OR (HL)
		JR Z,DRAWC3
		LD A,B
		CP 3
		LD A,PR_COL			;√┌┘▓ █─├─▓▌┴ ┼█▌▐┼┬
		JR NZ,DRAWC2
		LD A,UPR_COL			;√┌┘▓ ▌▓▐⌠≥┘█▌┴ ┼█▌▐┼┬
DRAWC2		LD B,(HL)			;≤┬░┬█─
		DEC B
		INC B
		CALL NZ,COL_KVAD		;░┬▒▌┌─▓° √┌┘▓█▌┴ ┼┌─└░─▓
DRAWC3		POP HL
		POP DE
		POP BC
		RET

PRINT_EMPTY	PUSH BC
		LD B,0X10
PRINT_EMPTY1	LD A," "
		CALL PRTSYMTXTMODE
		DJNZ PRINT_EMPTY1
		POP BC
		RET

PRT4TXTMODE	LD A,(HL)
		INC HL
		AND A
		RET Z
		CALL PRTSYM4TXTMOD
		JR PRT4TXTMODE

PRTSYM4TXTMOD	CP 0X20
		JP NC,PRTSYMTXTMODE
		CP 0X16
		RET NZ
		LD E,(HL)
		INC HL
		LD D,(HL)
		INC HL
		RET

SYSTEM_KEYS	LD DE,(PC_KEY_X+0X2E)*0X100+PC_KEY_Y+1
		LD BC,0X1203
		LD A,%01000011

COL_KVAD	PUSH DE
		PUSH BC	
COL_KVAD2	PUSH DE
		PUSH BC
COL_KVAD1	CALL SET_COL
		DJNZ COL_KVAD1
		POP BC
		POP DE
		INC E
		DEC C
		JR NZ,COL_KVAD2
		POP BC
		POP DE
		RET

SET_COL		PUSH DE
		PUSH BC
		PUSH AF
		LD A,0X1B
		CALL SET_7FFD
		LD L,E
		LD A,D
		LD H,0
		LD D,H
		ADD HL,HL
		ADD HL,HL
		ADD HL,HL
		ADD HL,HL
		ADD HL,HL
		ADD HL,HL
		LD E,A
		SRL E
		ADD HL,DE
		AND 1
		RRCA
		RRCA
		RRCA
		OR 0XC1
		LD D,A
		LD E,0XC0
		ADD HL,DE
		POP AF
		PUSH AF
		LD (HL),A
		LD A,0X1F
		CALL SET_7FFD
		POP AF
		POP BC
		POP DE
		INC D
		RET

